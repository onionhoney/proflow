{
  "manifest_version": 2,

  "name": "Proflow",
  "description": "A minimal distraction search log and notebook that syncs with Evernote",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
